<android.support.constraint.ConstraintLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/llDialerTab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context="org.sherman.android.theperfectshell.Fragments.DialerFragment">

        <!-- TODO: Update blank fragment layout -->
        <TextView
            android:id="@+id/tvDialerTab"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:textColor="@color/wallet_secondary_text_holo_dark"
            android:textSize="30sp"
            android:textStyle="bold"
            android:text="@string/hello_blank_fragment" />

    </LinearLayout>
    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_launcher_foreground"
        android:id="@+id/fabFAB1"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="146dp"
        android:layout_marginRight="146dp"
        android:clickable="true"
        android:onClick="dialOut"
        app:backgroundTint="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"

        />

</android.support.constraint.ConstraintLayout>